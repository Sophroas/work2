// 随机测试用例1: 复杂数学计算和循环
int power(int base, int exp) {
    int result = 1;
    int i = 0;
    while (i < exp) {
        result = result * base;
        i = i + 1;
    }
    return result;
}

int sum_sequence(int n) {
    int total = 0;
    int i = 1;
    while (i <= n) {
        if (i % 2 == 0) {
            total = total + i * 2;
        } else {
            total = total + i;
        }
        i = i + 1;
    }
    return total;
}

int main() {
    int x = 3;
    int y = 4;
    int z = power(x, y);  // 3^4 = 81
    int sum = sum_sequence(10);  // 计算序列和
    
    if (z > 50) {
        return z + sum;
    } else {
        return z - sum;
    }
}
