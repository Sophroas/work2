// 随机测试用例2: 嵌套控制结构和复杂逻辑
int fibonacci(int n) {
    if (n <= 0) {
        return 0;
    }
    if (n == 1) {
        return 1;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int check_prime(int num) {
    if (num <= 1) {
        return 0;
    }
    if (num == 2) {
        return 1;
    }
    
    int i = 2;
    while (i * i <= num) {
        if (num % i == 0) {
            return 0;
        }
        i = i + 1;
    }
    return 1;
}

int main() {
    int count = 0;
    int i = 1;
    
    // 计算前20个数中有多少个质数
    while (i <= 20) {
        if (check_prime(i)) {
            count = count + 1;
            
            // 如果是质数且是奇数，额外加分
            if (i % 2 == 1) {
                count = count + fibonacci(3);  // 额外加2
            }
        }
        i = i + 1;
    }
    
    return count;
}
